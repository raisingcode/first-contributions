{
 "properties": {
  "displayName": "Deny-Hybrid-JasonTest",
  "policyType": "Custom",
  "mode": "All",
  "metadata": {
   "createdBy": "b837ff0b-33ca-4aa1-8005-3a488b5acbd2",
   "createdOn": "2022-04-15T21:21:00.5753438Z",
   "updatedBy": "b837ff0b-33ca-4aa1-8005-3a488b5acbd2",
   "updatedOn": "2022-04-15T21:58:00.0000174Z"
  },
  "parameters": {},
  "policyRule": {
   "if": {
    "allOf": [
     {
      "field": "type",
      "in": [
       "Microsoft.Compute/virtualMachines",
       "Microsoft.Compute/VirtualMachineScaleSets"
      ]
     },
     {
      "field": "Microsoft.Compute/licenseType",
      "exists": true
     },
     {
      "not": {
       "allOf": [
        {
         "field": "Microsoft.Compute/imageOffer",
         "in": [
          "Windows-10"
         ]
        }
       ]
      }
     }
    ]
   },
   "then": {
    "effect": "deny"
   }
  }
 },
